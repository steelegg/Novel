<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fanchen.mapper.BookMapper">
    <select id="get" parameterType="int" resultType="Book">
        select * from book_date where id=#{id}
    </select>
    <insert id="insert" parameterType="Book">
        INSERT INTO  book_date
        (book_name,author,chapter_number,collection,recommended,last_update_time)VALUE
        (#{book_name},#{author},#{chapter_number},#{collection},#{recommended},#{last_update_time})
    </insert>


</mapper>